  "presets": {
    "base-elements": {
      "wrapper": {
        "position": "absolute",
        "width": "200px",
        "height": "200px",
        "border-radius": "10px",
        "background-color": "hsl(0, 0%, 50%)"
      },
      "image": {
        "position": "absolute",
        "display": "block",
        "width": "200px",
        "height": "200px",
        "border-radius": "10px",
        "overflow": "hidden",
        "object-fit": "cover",
        "_content": {
          "type": "img",
          "default": "https://studio-one-assets.studio-one.workers.dev/asset/fefc7b281fa5200a9bc1eff0402e9d7d036c5d867012c56c38da8637fe034aeb"
        }
      },
      "text": {
        "position": "absolute",
        "cursor": "text",
        "user-select": "text",
        "-webkit-user-select": "text",
        "_content": "Studio ONE",
        "_editable": true
      },
      "text-md": {
        "position": "absolute",
        "padding": "1.5rem",
        "background-color": "hsl(0, 0%, 98%)",
        "border": "1px solid hsl(0, 0%, 90%)",
        "border-radius": "8px",
        "_content": "# Welcome to Studio ONE\n\nA **unified design system** that brings together:\n- Single element philosophy\n- Theme-driven styling\n- Content-first approach",
        "_markdown": true
      }
    },
    "looks": {
      "primary": {
          "background-color": "hsl(340, 37%, 52%)",
          "color": "hsl(0, 0%, 100%)",
          "border-color": "hsl(340, 37%, 35%)",
          "outline-color": "hsl(340, 37%, 35%)",
          "fill": "hsl(0, 0%, 100%)",
          "stroke": "hsl(0, 0%, 60%)",
          "caret-color": "hsl(0, 0%, 100%)",
          "accent-color": "hsl(340, 37%, 35%)"
        },
        "test-color": {
          "background-color": "hsl(33, 44%, 51%)",
          "color": "hsl(0, 0%, 100%)"
        },
        "neutral-dark": {
          "background-color": "hsl(0, 0%, 15%)",
          "color": "hsl(0, 0%, 100%)",
          "border-color": "hsl(0, 0%, 30%)",
          "outline-color": "hsl(0, 0%, 30%)",
          "fill": "hsl(0, 0%, 35%)",
          "stroke": "hsl(0, 0%, 35%)",
          "caret-color": "hsl(0, 0%, 50%)",
          "accent-color": "hsl(0, 0%, 30%)"
        },
        "neutral-light": {
          "background-color": "hsl(0, 0%, 100%)",
          "color": "hsl(0, 0%, 10%)",
          "border-color": "hsl(0, 0%, 70%)",
          "outline-color": "hsl(0, 0%, 70%)",
          "fill": "hsl(0, 0%, 35%)",
          "stroke": "hsl(0, 0%, 35%)",
          "caret-color": "hsl(0, 0%, 50%)",
          "accent-color": "hsl(0, 0%, 70%)"
        },
        "ghost": {
          "background-color": "hsl(0, 0%, 80%)",
          "color": "hsl(0, 0%, 70%)",
          "border-color": "hsl(0, 0%, 80%)",
          "outline-color": "hsl(0, 0%, 80%)",
          "fill": "hsl(0, 0%, 90%)",
          "stroke": "hsl(0, 0%, 80%)",
          "caret-color": "hsl(0, 0%, 70%)",
          "accent-color": "hsl(0, 0%, 70%)"
        },
        "gradient-sunset": {
          "background": "linear-gradient(135deg, hsl(340, 37%, 52%) 0%, hsl(33, 44%, 51%) 100%)",
          "color": "hsl(0, 0%, 100%)"
        },
        "gradient-aurora": {
          "background": "linear-gradient(45deg, hsl(340, 37%, 42%) 0%, hsl(340, 37%, 52%) 25%, hsl(33, 44%, 51%) 75%, hsl(33, 44%, 61%) 100%)",
          "color": "hsl(0, 0%, 100%)"
        },
        "gradient-radial": {
          "background": "radial-gradient(circle at 30% 30%, hsl(33, 44%, 61%) 0%, hsl(340, 37%, 52%) 60%, hsl(340, 37%, 32%) 100%)",
          "color": "hsl(0, 0%, 100%)"
        }
    }
  },
  "structure": {
    "wrapper-element": {
      "type": "one",
      "presets": "wrapper gradient-sunset",
      "data-element-type": "wrapper"
    },
    "text-element": {
